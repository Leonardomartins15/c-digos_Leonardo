   "source": [
    "# estou importando o pyautogui e apelidando ele de aut\n",
    "import pyautogui as aut \n",
    "\n",
    "# pressiono a tecla de atalho win +r\n",
    "aut.hotkey(\"win\", \"r\")\n",
    "# escrever notepad\n",
    "aut.write(\"notepad\", interval=0.7)\n",
    "# espera 1 segundo\n",
    "aut.sleep(1)\n",
    "# aperta o enter\n",
    "aut.press(\"Enter\")"
   ]
  }
